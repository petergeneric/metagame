			},
			{
				"name": "William Coffin",
				"maploc": "D-2",
				"description": "A shady character found at the Fleetwood Diner who has connections to unsavory places. Will provide information for a price."
			},
			{
				"name": "Nicolas Staltonstall",
				"maploc": "D-21",
				"description": "Overworked civil servant at Arkham Town Hall who keeps legal documents like birth, marriage, and death certificates, as well as property deeds."
			},
			{
				"name": "Inspector Garrison",
				"maploc": "D-32",
				"description": "Head detective at the Arkham Police Station."
			},
			{
				"name": "Herbert Corbett",
				"maploc": "D-45",
				"description": "Local criminologist with unmatched forensic expertise, provided he has evidence to examine."
			},
			{
				"name": "Arlo Hoppin",
				"maploc": "D-34",
				"description": "Friendly and verbose dispatcher/cabbie for the Arkham Yellow Cab Company who meets many people through his job. Also a talented musician."
			},
			{
				"name": "Dorothy Davis",
				"maploc": "L-1",
				"description": "Works at the Arkham Historical Society with extensive knowledge of family history and genealogical records that can reveal surprising family connections."
			},
			{
				"name": "Pasquale Fenton",
				"maploc": "L-4",
				"description": "Arkham's resident occultist and spiritualist with extensive knowledge of the Great Old Ones. Sometimes works with investigators despite Armitage's disapproval."
			},
			{
				"name": "Sebastian Lyman",
				"maploc": "L-18",
				"description": "A mole within the Order of the Silver Twilight who secretly works for Armitage, providing information on the cult and its leader, Carl Sandford."
			},
			{
				"name": "Donnie Clarke",
				"maploc": "N-28",
				"description": "Star reporter at the Arkham Advertiser who has a zeal for grabbing attention and seeks the truth."
			},
			{
				"name": "Ambrose Checkley",
				"maploc": "U-18",
				"description": "High-society mogul and notorious gossip who can provide information about the privileged class and entertainment circles."
			}
